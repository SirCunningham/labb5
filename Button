package labb5;

import javax.swing.*;
import java.awt.image.*;
import java.net.*;
import java.io.*;
import javax.imageio.ImageIO;
import java.awt.geom.*;
import java.util.*;

public class Button extends JButton {

    private URL url;
    private LinkedList<URL> listOfUrls;

    public void addUrl(URL url) {
        listOfUrls.add(url);
    }

    public void remove() {
        listOfUrls.remove(listOfUrls.size());
    }

    public LinkedList<URL> getListOfUrls() {
        return listOfUrls;
    }
}
